//  Thanks to Bina
{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "source": "~/.config/fastfetch/logo.txt",
        "padding": {
            "top": 3
        },
        "color": {"1": "#125656"}
    },
    "display": {
        // "separator": " ➜  "
        "separator": "  "
    },
    "modules": [
        "break",
        "break",
        {
            "type": "os",
            "key": " ",
            "format": "{name}",
            "keyColor": "34", // = color1
        },
        {
            "type": "shell",
            "key": " ",
            "format": "{pretty-name}",
            "keyColor": "33",
        },
        {
            "type": "wm",
            "key": "󰙀 ",
            "format": "{pretty-name}",
            "keyColor": "35",
        },
        {
            "type": "cpu",
            "format": "{1}",
            "key": " ",
            "keyColor": "32",
        },
        {
            "type": "gpu",
            "format": "{name}",
            "key": "󰢮 ",
            "keyColor": "36",
            "hideType": "integrated"
        },
        {
            "type": "users",
            "format": "{name}",
            "key": " ",
            "keyColor": "37",
        }
        
    ]
}